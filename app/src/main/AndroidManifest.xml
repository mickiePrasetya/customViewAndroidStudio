<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.web.owlstudio.trainingbasic2">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CustomListViewActivity" />
        <activity android:name=".WebViewActivity" />
        <activity android:name=".CustomWebViewActivity" />
        <activity android:name=".CustomSpinnerActivity" />
        <activity android:name=".Wiki"></activity>
        <activity
            android:name=".DetailBuahActivity"
            android:label="@string/title_activity_detail_buah"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>